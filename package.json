{
  "name": "react-redux-flow-tools",
  "version": "0.0.9",
  "description": "Assortment of tools useful in combining React/Redux/Flow and functional programming in general",
  "main": "lib/index.js",
  "scripts": {
    "eslint": "./node_modules/eslint-watch/bin/esw src/",
    "eslint:watch": "./node_modules/eslint-watch/bin/esw -w src/",
    "flow": "./node_modules/flow-bin/cli.js",
    "flow:watch": "flow-watch",
    "flow-typed": "./node_modules/flow-typed/dist/cli.js update",
    "start": "node ./node_modules/npm-run-all/bin/npm-run-all -p eslint:watch",
    "build": "node ./node_modules/npm-run-all/bin/npm-run-all flow-typed eslint flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chjdev/react-redux-flow-tools.git"
  },
  "keywords": [
    "react",
    "redux",
    "flow",
    "functional"
  ],
  "author": "Christian Junker",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/chjdev/react-redux-flow-tools/issues"
  },
  "homepage": "https://github.com/chjdev/react-redux-flow-tools#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.4.0",
    "flow-watch": "^1.1.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  }
}
